	@echo "  {" >> $(PACKAGE).rc
	@echo "    BLOCK \"040904E4\"" >> $(PACKAGE).rc
	@echo "    {" >> $(PACKAGE).rc
	@echo "      VALUE \"CompanyName\", \"LGames - Michael Speck\"" >> $(PACKAGE).rc
	@echo "      VALUE \"FileDescription\", \"@appname@\"" >> $(PACKAGE).rc
